    "auth0": "^2.17.0",
    "auth0-js": "^9.10.2",
    "axios": "^0.18.0",
    "colors": "^1.3.3",
    "express": "^4.16.3",
    "express-session": "^1.16.1",
    "if-env": "^1.0.4",
    "mongoose": "^5.3.16",
    "node-sass": "^4.12.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.0",
    "reactstrap": "^8.0.0",
    "sass": "^1.19.0"
